PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% %predicate% %object% .} where {%owner% rdf:type mse:requestor. %owner% mse:performs mse:add.}
PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% %predicate% %object% .} where {%owner% rdf:type mse:requestor. %owner% mse:performs mse:read. %subject% %predicate% %object% . }
PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% %predicate% %object% .} where {?req rdf:type mse:requestor.  ?req mse:performs mse:read.  ?req <%namespace%affiliation> ?group. %subject% <%namespace%affiliation> ?group. %subject% %predicate% %object%.}
PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% %predicate% %object% .} where {?req rdf:type mse:requestor.  ?req mse:performs mse:add.  %owner% mse:knows ?req.}
PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% <http://yarta.gforge.inria.fr/ontologies/mse.rdf#email> %object%} where {?req rdf:type mse:requestor.  ?req mse:performs mse:add.  ?req <%namespace%affiliation> ?group. %subject% <%namespace%affiliation> ?group. %subject% mse:knows ?req.}