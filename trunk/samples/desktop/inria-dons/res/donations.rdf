<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE rdf:RDF [
  <!ENTITY dons "http://yarta.gforge.inria.fr/ontologies/donations.rdf#">
  <!ENTITY mse "http://yarta.gforge.inria.fr/ontologies/mse.rdf#">
  <!ENTITY owl "http://www.w3.org/2002/07/owl#">
  <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#">
  <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#">
  <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#">
]>
<rdf:RDF xml:base="&dons;"
		 xmlns:dons="&dons;"
         xmlns:mse="&mse;"
         xmlns:owl="&owl;"
         xmlns:rdf="&rdf;"
         xmlns:rdfs="&rdfs;">
		 
		 <!-- Ontology Information -->
		 <owl:Ontology rdf:about="">
			<owl:imports>
				<owl:Ontology rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#"/>
			</owl:imports>
		</owl:Ontology>
		
		<!-- Classes -->
		<owl:Class rdf:about="#Announcement">
			<rdfs:subClassOf rdf:resource="&mse;Content" />
		</owl:Class>

		<owl:Class rdf:about="#Request">
			<rdfs:subClassOf rdf:resource="#Announcement" />
		</owl:Class>
		
		<owl:Class rdf:about="#Donation">
			<rdfs:subClassOf rdf:resource="#Announcement" />
		</owl:Class>
		
		<owl:Class rdf:about="#Sale">
			<rdfs:subClassOf rdf:resource="#Announcement" />
		</owl:Class>
		
		<owl:Class rdf:about="#Picture">
			<rdfs:subClassOf rdf:resource="&mse;Content" />
		</owl:Class>

		<owl:Class rdf:about="#Category">
			<rdfs:subClassOf rdf:resource="&mse;Topic" />
		</owl:Class>
		
		<!-- Data type properties -->
		<owl:DatatypeProperty rdf:about="#description">
			<rdfs:domain rdf:resource="#Announcement" />
			<rdfs:range rdf:resource="&xsd;string"/>
		</owl:DatatypeProperty>
		
		<owl:DatatypeProperty rdf:about="#time">
			<rdfs:domain rdf:resource="#Announcement" />
			<rdfs:range rdf:resource="&xsd;long"/>
		</owl:DatatypeProperty>
		
		<owl:DatatypeProperty rdf:about="#phone">
			<rdfs:domain rdf:resource="&mse;Person" />
			<rdfs:range rdf:resource="&xsd;string" />
		</owl:DatatypeProperty>
		
		<owl:DatatypeProperty rdf:about="#price">
			<rdfs:domain rdf:resource="#Announcement" />
			<rdfs:range rdf:resource="&xsd;float" />
		</owl:DatatypeProperty>
		
		<!-- Object type properties -->
		<owl:ObjectProperty rdf:about="#picture"
                      rdfs:label="picture">
			<rdfs:domain rdf:resource="#Announcement"/>
			<rdfs:range rdf:resource="#Picture"/>
		</owl:ObjectProperty>

		<owl:ObjectProperty rdf:about="#category"
                      rdfs:label="category">
			<rdfs:domain rdf:resource="#Announcement"/>
			<rdfs:range rdf:resource="#Category"/>
		</owl:ObjectProperty>

		<owl:ObjectProperty rdf:about="#hiddenBy"
                      rdfs:label="hiddenBy">
			<rdfs:domain rdf:resource="#Announcement"/>
			<rdfs:range rdf:resource="&mse;Person"/>
		</owl:ObjectProperty>
		
		<!-- Predefined categories -->
		<rdf:Description rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#Kitchen">
			<rdf:type rdf:resource="#Category"/>
		</rdf:Description>

		<rdf:Description rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#Games">
			<rdf:type rdf:resource="#Category"/>
		</rdf:Description>

		<rdf:Description rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#Furniture">
			<rdf:type rdf:resource="#Category"/>
		</rdf:Description>

		<rdf:Description rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#Leisure">
			<rdf:type rdf:resource="#Category"/>
		</rdf:Description>

		<rdf:Description rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#Animals">
			<rdf:type rdf:resource="#Category"/>
		</rdf:Description>

		<rdf:Description rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#Gardening">
			<rdf:type rdf:resource="#Category"/>
		</rdf:Description>

		<rdf:Description rdf:about="http://yarta.gforge.inria.fr/ontologies/mse.rdf#Misc">
			<rdf:type rdf:resource="#Category"/>
		</rdf:Description>

		<!-- TODO: add sanjay with his picture -->
		
		<!-- add groups. add people. later. first encryption ndk simple.-->
</rdf:RDF>
