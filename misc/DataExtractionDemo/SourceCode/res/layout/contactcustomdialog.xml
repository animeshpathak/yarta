<?xml version="1.0" encoding="utf-8"?>
 	
<RelativeLayout
				xmlns:android="http://schemas.android.com/apk/res/android"
				android:layout_width="fill_parent"
			    android:background="#000"
				android:layout_height="wrap_content"
				android:padding="6dip">
  		        
		<TextView
				android:id="@+id/customDialogNameTop"
				android:layout_marginLeft="6dip"
				android:layout_width="fill_parent"
				android:layout_height="26dip"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:gravity="top"
				 />
		<!-- 
		<TextView
				android:id="@+id/customDialogNameBottom"
				android:layout_below="@id/customDialogNameTop"
				android:layout_marginLeft="12dip"
				android:layout_width="fill_parent"
				android:layout_height="26dip"
				android:gravity="top"
				 />
	
		 -->
		 
		<TextView
				android:id="@+id/customDialogPhoneTop"
				android:layout_marginLeft="6dip"
				android:layout_width="fill_parent"
				android:layout_height="26dip"
				android:layout_below="@id/customDialogNameTop"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:gravity="bottom"
				 />
		
		
		<TextView
				android:id="@+id/customDialogPhoneBottom"
				android:layout_width="fill_parent"
				android:layout_below="@id/customDialogPhoneTop"
				android:layout_marginLeft="12dip"
				android:layout_height="wrap_content"
				android:gravity="top"
				 />
	
		
		<TextView
				android:id="@+id/customDialogEmailTop"
				android:layout_marginLeft="6dip"
				android:layout_width="fill_parent"
				android:layout_below="@id/customDialogPhoneBottom"
				android:layout_height="26dip"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:gravity="bottom"
				 />
					 
			<TextView
				android:id="@+id/customDialogEmailBottom"
				android:layout_width="fill_parent"
				android:layout_marginLeft="12dip"
				android:layout_below="@id/customDialogEmailTop"
				android:layout_height="wrap_content"
				android:gravity="top"
				 />
			 
				 
	
	</RelativeLayout>
