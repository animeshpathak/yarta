PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% %predicate% %object% .} where {%owner% rdf:type mse:requestor. %owner% mse:performs mse:read. %subject% %predicate% %object% . }
PREFIX ns:<%namespace%> PREFIX mse-extended:<http://yarta.gforge.inria.fr/ontologies/conference/mse-extended.rdf#> PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% %predicate% %object%} where {?req rdf:type mse:requestor.  ?req mse:performs mse:read.  %subject% mse:knows ?req. %subject% %predicate% %object%.}
PREFIX ns:<%namespace%> PREFIX mse-extended:<http://yarta.gforge.inria.fr/ontologies/conference/mse-extended.rdf#> PREFIX mse:<http://yarta.gforge.inria.fr/ontologies/mse.rdf#> PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>  PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#> CONSTRUCT {%subject% %predicate% %object%} where {%subject% rdf:type mse:requestor. %subject% mse:performs mse:remove. %subject% %predicate% %object%.   }